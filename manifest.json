{
  "manifest_version": 2,
  "name": "Toggle.me - WordPress Theme/Plugin detector",
  "description": "Find out which WordPress theme and plugins your favorite sites are using.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "toggle.html",
    "default_title": "Toggle.me detector"
  },
  "permissions": [
    "activeTab",
    "https://alpha.toggle.me/",
    "http://toggle.app/",
    "https://toggle.me/",
    "https://ajax.googleapis.com/",
    "https://cdnjs.cloudflare.com",
    "https://fonts.googleapis.com"
  ]
}